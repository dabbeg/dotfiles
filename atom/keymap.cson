'atom-text-editor.vim-mode.insert-mode':
  'j j': 'vim-mode:reset-normal-mode'

'atom-text-editor.vim-mode:not(.insert-mode)':
  'j': 'vim-mode:move-left'
  'k': 'vim-mode:move-down'
  'l': 'vim-mode:move-up'
  ';': 'vim-mode:move-right'
  'ctrl-c': 'core:copy'
  '1': 'tree-view:toggle-focus'
  '2': 'pane:show-next-item'

#'atom-workspace':
#  '1': 'tree-view:toggle-focus'
#  '2': 'pane:show-next-item'

'.tree-view':
  'j': 'tree-view:collapse-directory'
  'k': 'core:move-down'
  'l': 'core:move-up'
  ';': 'tree-view:expand-directory'
  '1': 'tree-view:toggle-focus'

#https://github.com/atom/vim-mode/blob/master/keymaps/vim-mode.cson
